    <script>
      var $el = $('.experience-features-item');
      var $elPop = $('.experience-features-popup-seg');
      var $elClosePop = $('.experience-features-popup-seg .close-seg');

      $el.on('click', function (e) {
        var $self = $(this);

        e.preventDefault();
        e.stopPropagation();

        var time = 0;
        if ($elPop.hasClass('is-active')) {
          time = 100;
        }

        $elPop.addClass('is-unactive');
        setTimeout(function (e) {
          $elPop.removeClass('is-active');
          $elPop.removeClass('is-unactive');
          $elPop.removeClass('odd');

          var type = $self.data('type');
          var index = $self.data('index');

          var classAppend = '';
          if (type == 2) {
            classAppend = 'odd';
          }

          var $elTitle = $self.find('.item-title');
          if ($elTitle.length < 0) {
            return;
          }

          var $elPopupTitle = $elPop.find('.item-title');
          $elPopupTitle.text($elTitle.text());

          var $elDesc = $self.find('.item-des');
          if ($elDesc.length < 0) {
            return;
          }

          var $elPopupBody = $elPop.find(
            '.experience-features-popup-seg__body'
          );
          $elPopupBody.text($elDesc.text());

          $elPop.removeClass('is-unactive');
          $elPop.addClass('is-active');
          $elPop.css('top', index * 49 + 'px');
          if (classAppend) {
            $elPop.addClass(classAppend);
          }
        }, time);
      });

      $elPop.on('click', function (e) {
        e.stopPropagation();
      });

      $elClosePop.on('click', function (e) {
        $elPop.addClass('is-unactive');
        setTimeout(function (e) {
          $elPop.removeClass('is-active');
          $elPop.removeClass('is-unactive');
          $elPop.removeClass('odd');
        }, 100);
      });

      $(document).on('click', function (e) {
        $elPop.addClass('is-unactive');
        setTimeout(function (e) {
          $elPop.removeClass('is-active');
          $elPop.removeClass('is-unactive');
          $elPop.removeClass('odd');
        }, 100);
      });
    </script>
